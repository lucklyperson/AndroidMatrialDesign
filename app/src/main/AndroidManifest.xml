<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.androidmatrialdesignproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppBaseTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AppBarLayoutActivity" />
        <activity android:name=".CoordinatorActivity" />
        <activity android:name=".TextInputLayoutActivity" />
        <activity android:name=".TableLayoutActivity" />
        <activity android:name=".CollaspingToolbarActivity"></activity>
    </application>

</manifest>